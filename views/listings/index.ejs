<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WonderDnd</title>
</head>
<body>
    <h3>All listings</h3>
    
    <% if(locals.data_Collections){ %>
        <form method="get" action="/listings/new">
        <button>Create new Listing</button>
        </form>
    <ul>
        <% data_Collections.forEach((data)=>{ %>
        <li><a href="/listings/<%=data._id%>"><%=data.title %></a></li>
    <% }) %>
    </ul>
    <% } %>
</body>
</html>

